        <TextBlock Grid.Row="0" Grid.Column ="8" Text="16" />
        <TextBlock Grid.Row="0" Grid.Column ="9" Text="18" />
        <TextBlock Grid.Row="0" Grid.Column ="10" Text="20" />
        <DockPanel Grid.Row="0" Grid.Column ="11">
            <TextBlock Text="22" HorizontalAlignment="Left" />
            <TextBlock Text="24" HorizontalAlignment="Right" />
        </DockPanel>

